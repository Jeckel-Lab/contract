{
  "name": "jeckel-lab/contract",
  "description": "Contract / Interfaces used by other packages and DDD projects",
  "type": "library",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Julien Mercier-Rojas",
      "email": "julien@jeckel-lab.fr"
    }
  ],
  "require": {
    "php": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "JeckelLab\\Contract\\": "src/"
    }
  },
  "require-dev": {
    "phpro/grumphp": "^1.5",
    "vimeo/psalm": "^4.12",
    "phpstan/phpstan": "^1.1",
    "phpmd/phpmd": "^2.10",
    "squizlabs/php_codesniffer": "^3.6",
    "roave/security-advisories": "dev-master",
    "maglnet/composer-require-checker": "^3.4"
  },
  "suggest": {
    "vimeo/psalm": "Using psalm with this 'contract' will enable immutability and strong typing verification"
  }
}
